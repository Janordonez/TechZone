<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE application SYSTEM "dtds/application.dtd">

<application name="TechZone"> 
	
	<!-- 
	A default module for each entity is assumed with the
	controllers on <default-module/>
	-->
	<default-module>		  
		<controller name="Typical"/>		
	</default-module>


	<module name="Factura">
		<model name = "Factura"/>
		<controller name="MYCRUD"/>
	</module>

	<module name="Producto">
		<model name="Producto"/>
		<controller name="Producto"/>
	</module>

	<!-- 
	You can define additional modules, for example:	  
	<module name="OnlyReadTeacher">
		<model name="Teacher"/>
		<controller name="Print"/>
	</module>		
	-->
	
</application>
